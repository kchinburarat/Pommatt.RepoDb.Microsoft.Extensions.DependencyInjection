<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<Description>RepoDB extensions for ASP.NET Core</Description>
		<Copyright>Copyright keattisak Chinburart</Copyright>
		<TargetFrameworks>netstandard2.0</TargetFrameworks>
		<AssemblyName>Pommatt.RepoDb.Microsoft.Extensions.DependencyInjection</AssemblyName>
		<PackageId>Pommatt.RepoDb.Microsoft.Extensions.DependencyInjection</PackageId>
		<PackageTags></PackageTags>
		<SignAssembly>true</SignAssembly>
		<PublishRepositoryUrl>true</PublishRepositoryUrl>
		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<EmbedUntrackedSources>true</EmbedUntrackedSources>
		<Deterministic>true</Deterministic>
		<RepositoryUrl>https://github.com/kchinburarat/Pommatt.RepoDb.Microsoft.Extensions.DependencyInjection</RepositoryUrl>
	</PropertyGroup>


	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Http.Abstractions" Version="2.2.0" />
		<PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="5.0.0" />
		<PackageReference Include="RepoDb" Version="1.12.9" />
	</ItemGroup>

</Project>
